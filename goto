#include <iostream>
using namespace std;

int main () {
	//membuat program goto
	char loncat = 0;
	
	cout << " masukkan huruf y untuk meloncat " <<endl;
	cout << " masukkaan huruf n untuk tidak meloncat " <<endl;
	cin >> loncat;
	
	cout << " 1 " <<endl;
	cout << " 2 " <<endl;
		if ( loncat == 'y' ) goto loncatan;
	cout << " 3 " <<endl;
		loncatan:
	cout << " 4 " <<endl;
	cout << " 5 " <<endl;
	
	return 0;
}
